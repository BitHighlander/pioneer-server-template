{
  "name": "service-2",
  "version": "1.0.0",
  "scripts": {
    "refresh": "rm -Rf node_modules dist api && yarn",
    "yarn": "yarn",
    "build:web": "echo lol",
    "build:routes": "mkdir -p src/routes && tsoa routes --configuration tsoa.json",
    "build:swagger": "mkdir -p api && mkdir -p api/dist && tsoa swagger --configuration tsoa.json",
    "build:ts": "tsc -p src",
    "build:all": "npm run build:routes && npm run build:swagger && npm run build:ts && npm run build:web",
    "start": "node dist/main.js",
    "start-dev": "yarn build:all && yarn start",
    "build:routes:prod": "mkdir -p src/routes && tsoa routes --configuration tsoaProd.json",
    "build:swagger:prod": "mkdir -p api && mkdir -p api/dist && tsoa swagger --configuration tsoaProd.json",
    "build:all:prod": "npm run build:routes:prod && npm run build:swagger:prod && npm run build:ts && npm run build:web",
    "docker:build": "docker build -t pioneer/pioneer-server-v2:latest .",
    "docker:tag": "docker tag pioneer/pioneer-server-v2:latest registry.digitalocean.com/pioneer/pioneer/pioneer-server-v2:latest",
    "docker:run": "docker run --env-file=../../.env pioneer/pioneer-server-v2:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/pioneer-server-v2:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "dependencies": {
    "@pioneer-platform/default-mongo": "latest",
    "@pioneer-platform/default-redis": "latest",
    "@pioneer-platform/eth-network": "^8.1.76",
    "@pioneer-platform/loggerdog": "^8.1.31",
    "@pioneer-platform/markets": "^8.1.46",
    "@pioneer-platform/pioneer": "^8.2.26",
    "@pioneer-platform/pioneer-caip": "^8.1.79",
    "@pioneer-platform/pioneer-coins": "^8.1.74",
    "@pioneer-platform/pioneer-network": "8.1.51",
    "@pioneer-platform/pioneer-types": "^8.1.55",
    "@pioneer-platform/redis-queue": "^8.1.24",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/node": "^12.12.44",
    "axios": "^1.5.1",
    "body-parser": "^1.19.0",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "method-override": "^3.0.0",
    "randomstring": "^1.3.0",
    "rate-limiter-flexible": "^2.1.7",
    "redis": "^4.6.7",
    "short-uuid": "^4.2.2",
    "socket.io": "^4.7.2",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "3.2.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2"
  }
}
